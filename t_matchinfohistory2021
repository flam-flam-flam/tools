CREATE TABLE `t_matchinfohistory2021` (
  `Id` bigint NOT NULL AUTO_INCREMENT COMMENT '主键id',
  `Leaguename` varchar(64) DEFAULT NULL COMMENT '联赛名称',
  `Matchid` varchar(20) DEFAULT NULL COMMENT '比赛id',
  `Matchtime` datetime DEFAULT NULL COMMENT '比赛日期',
  `Matchtimestring` varchar(36) DEFAULT NULL COMMENT '比赛时间',
  `Status` varchar(16) DEFAULT NULL COMMENT '比赛状态',
  `Hostrank` varchar(20) DEFAULT NULL COMMENT '主队排名',
  `Hostname` varchar(64) DEFAULT NULL COMMENT '主队名称',
  `Customerrank` varchar(20) DEFAULT NULL COMMENT '客队排名',
  `Customername` varchar(64) DEFAULT NULL COMMENT '客队名称',
  `Score` varchar(16) DEFAULT NULL COMMENT '比分',
  `Goal` int DEFAULT '0' COMMENT '进球数',
  `Halfscore` varchar(16) DEFAULT NULL COMMENT '半场比分',
  `Halfgoal` int DEFAULT '0' COMMENT '半场进球数',
  `Aisa` varchar(20) DEFAULT NULL COMMENT '亚盘初盘',
  `Overunder` varchar(20) DEFAULT NULL COMMENT '大小球初盘',
  `Mark` varchar(20) DEFAULT NULL COMMENT '备注',
  `Premark` varchar(20) DEFAULT NULL COMMENT '预测结果',
  `Premarks` varchar(255) DEFAULT NULL COMMENT '预测结果集',
  `Preoverunders` varchar(255) DEFAULT NULL COMMENT '预测大小球',
  `Halfmark` varchar(20) DEFAULT NULL COMMENT '备注',
  `Prehalfmark` varchar(20) DEFAULT NULL COMMENT '预测结果',
  `Prehalfmarks` varchar(255) DEFAULT NULL COMMENT '预测结果集',
  `Prehalfoverunders` varchar(255) DEFAULT NULL COMMENT '预测大小球',
  `Pretime` datetime DEFAULT NULL COMMENT '创建时间',
  `Ext` varchar(255) DEFAULT NULL COMMENT '扩展',
  `Createtime` datetime DEFAULT now() COMMENT '创建时间',
  `Updatetime` datetime DEFAULT now() COMMENT '更新时间',
  PRIMARY KEY (`Id`),
  UNIQUE KEY `MatchId_idx` (`Matchid`),
  KEY `MatchTime_idx` (`Matchtime`),
  KEY `CreateTime_idx` (`Createtime`),
  KEY `LeagueName_idx` (`Leaguename`),
  KEY `HostName_idx` (`Hostname`),
  KEY `CustomerName_idx` (`Customername`),
  KEY `Aisa_idx` (`Aisa`),
  KEY `Overunder_idx` (`Overunder`)
) ENGINE=InnoDB AUTO_INCREMENT=502 DEFAULT CHARSET=utf8mb3 COMMENT='基本信息表';
